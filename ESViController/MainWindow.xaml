<Window x:Class="ESViController.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ESViController"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="listBox" Margin="0,0,180,0"/>
        <Button x:Name="buttonStart" Content="Start" HorizontalAlignment="Left" Margin="620,0,0,0" VerticalAlignment="Top" Height="68" Width="180" Click="buttonStart_Click"/>
        <Button x:Name="buttonEnd" Content="End" HorizontalAlignment="Left" Margin="620,85,0,0" VerticalAlignment="Top" Height="68" Width="180" Click="buttonEnd_Click"/>
        <Slider x:Name="slider" HorizontalAlignment="Left" Margin="625,170,0,0" VerticalAlignment="Top" Width="165" Maximum="1000"/>

    </Grid>
</Window>
